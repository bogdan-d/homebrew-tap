{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>ublue-os/renovate-config"
  ],
  "packageRules": [
    {
      "description": "Auto-merge PRs created by Homebrew bump workflow",
      "matchManagers": [
        "homebrew"
      ],
      "automerge": true,
      "automergeType": "pr",
      "matchUpdateTypes": [
        "bump"
      ]
    }
  ]
}